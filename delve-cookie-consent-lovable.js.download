            }
            
            .delve-categories-container {
                padding: 0 20px;
            }
            
            .delve-category-header {
                padding: 16px 0;
            }
            
            .delve-category-name {
                font-size: 0.9375rem;
            }
            
            .delve-category-description {
                font-size: 0.8125rem;
                padding-left: 26px;
                margin-bottom: 16px;
            }
            
            .delve-toggle {
                width: 44px;
                height: 24px;
                border-radius: 12px;
            }
            
            .delve-toggle-thumb {
                top: 2px;
                left: 2px;
                width: 20px;
                height: 20px;
            }
            
            .delve-toggle.enabled .delve-toggle-thumb {
