                padding: 16px 0 0 0;
                margin-top: 16px;
            }
            
            .delve-banner-brand-text {
                font-size: 0.625rem;
            }
            
            .delve-settings-footer {
                border-radius: 0;
                padding: 12px 20px;
            }
            
            .delve-settings-brand-text {
                font-size: 0.75rem;
            }
